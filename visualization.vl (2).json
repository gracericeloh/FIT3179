{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": "Import and Exports of Australia 2016",
  "width": 800,
  "height": 600,
  "projection": {"type": "equirectangular"},
  "data": {
    "url": "https://raw.githubusercontent.com/gracericeloh/FIT3179/main/Aust.json",
    "format": {"type": "topojson", "feature": "STE11aAust"}
  },
  "transform": [
    {
      "lookup": "properties.STATE_NAME",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/gracericeloh/FIT3179/main/2016_import_export.csv"
        },
        "key": "State",
        "fields": ["Expenditure in thousands"]
      }
    }
  ],
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {"field": "Expenditure in thousands", "type": "quantitative"},
    "tooltip": [
      {"field": "properties.STATE_NAME", "type": "nominal", "title": "Country"},
      {"field": "Expenditure in thousands", "type": "quantitative"}
    ]
  },
  "config": {}
}