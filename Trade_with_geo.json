{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "title": {
    "text": "Australia's trade types with Geographic partner",
    "fontSize": 20,
    "align": "center",
    "anchor": "middle",
    "lineHeight": 50
  },
  "width": {"step": 100},
  "height": 600,
  "data": {
    "url": "https://raw.githubusercontent.com/gracericeloh/FIT3179/main/2016_import_export.csv"
  },
  "transform": [
    {
      "calculate": "datum.Trade_type == 'Total Exports' ? 'Total Exports' : 'Total Imports'",
      "as": "Trade type"
    }
  ],
  "mark": "bar",
  "encoding": {
    "column": {
      "field": "Geo_1",
      "type": "nominal",
      "spacing": 10,
      "header": {"title": "", "titleFontSize": 15, "labelFontSize": 20}
    },
    "x": {
      "field": "Trade type",
      "axis": {
        "labelAngle": -35,
        "title": "",
        "labelFontSize": 15,
        "titleFontSize": 20
      }
    },
    "y": {
      "aggregate": "count",
      "field": "Trade type",
      "axis": {"grid": false, "labelFontSize": 15}
    },
    "color": {"field": "Trade type", "scale": {"range": ["#675193", "#ca8861"]}}
  },
  "config": {"view": {"stroke": "transparent"}, "axis": {"domainWidth": 1}}
}